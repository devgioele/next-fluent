{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "TypeScript Library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["esnext", "dom"],
    "target": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "jsx": "react"
  }
}
